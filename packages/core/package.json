{
  "name": "@candriajs/karin-plugin-git",
  "version": "1.4.0",
  "description": "一个karin的git平台仓库订阅推送",
  "keywords": [
    "karin-plugin",
    "karin-plugin-git"
  ],
  "homepage": "https://github.com/CandriaJS/karin-plugin-git",
  "bugs": {
    "url": "https://github.com/CandriaJS/karin-plugin-git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CandriaJS/karin-plugin-git.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "CandriaJS",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "app": "node --import tsx src/app.ts",
    "build": "tsc --noEmit && tsdown",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "watch": "cross-env EBV_FILE=\"development.env\" tsx watch --include \"src/**/*.ts\" src/app.ts"
  },
  "dependencies": {
    "@puniyu/component": "^0.6.0",
    "es-toolkit": "^1.44.0",
    "nipaw": "npm:@puniyu/nipaw@^1.9.5"
  },
  "devDependencies": {
    "@candriajs/template": "workspace:*",
    "@karinjs/plugin-puppeteer": "^1.0.21",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.9",
    "cross-env": "^7.0.3",
    "node-karin": "^1.14.2"
  },
  "karin": {
    "main": "src/index.ts",
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "engines": {
      "karin": ">=1.9.1"
    }
  }
}
